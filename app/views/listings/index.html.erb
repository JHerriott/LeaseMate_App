<h1>Listing listings</h1>

<table>
  <tr>
    <th>Address</th>
    <th>Listing Details</th>
    <th>Applications</th>
    <th>Invite</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.address %></td>
    <td><%= link_to 'Show Details', listing %></td>
    <td><%= link_to 'Applications', apps_url(:listing_id => listing.id) %></td>
    <td><%= link_to 'Invite Applicant', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %> | <%= link_to 'Delete', listing, confirm: 'Are you sure?', method: :delete %></td>
    <td></td>
    <td></td>
    <td></td>
  </tr>
<% end %>
</table>

<br />

