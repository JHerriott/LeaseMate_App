<h1>Listing apps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>SSN</th>
    <th>Driver's License #</th>
    <th>Phone</th>
    <th>Pets</th>
    <th>Present Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @apps.each do |app| %>
  <tr>
    <td><%= app.fname %> <%= app.lname %></td>
    <td><%= app.dob %></td>
    <td><%= app.ssn %></td>
    <td><%= app.d_lic_no %>, <%= app.d_lic_state %></td>
    <td><%= app.phone %></td>
    <td><%= app.pets %></td>
    <td><%= app.present_add %></td>
    <td><%= app.listing_id %></td>
    <td><%= app.renter_id %></td>
    <td><%= link_to 'Show', app %></td>
    <td><%= link_to 'Edit', edit_app_path(app) %></td>
    <td><%= link_to 'Destroy', app, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App', new_app_path %>
