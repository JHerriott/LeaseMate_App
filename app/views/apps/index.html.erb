<div class="content"><h3>Listing apps</h3></br>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>SSN</th>
    <th>Driver's License #</th>
    <th>Phone</th>
    <th>Pets</th>
    <th>Present Address</th>
	<th></th>
	<th></th>
	<th></th>
	<th></th>
	<th></th>
  </tr>

<% @apps.each do |app| %>
  <tr>
    <td><%= app.fname %> <%= app.lname %></td>
    <td><%= app.dob %></td>
    <td><%= app.ssn %></td>
    <td><%= app.d_lic_no %>, <%= app.d_lic_state %></td>
    <td><%= app.phone %></td>
    <td><%= app.pets %></td>
    <td><%= app.present_add %></td>
    <td><%= app.listing_id %></td>
    <td><%= app.renter_id %></td>
    <td><%= link_to 'Show', listing_app_url(@listing, app) %></td>
    <td><%= link_to 'Edit', edit_listing_app_path(@listing, app) %></td>
    <td><%= link_to 'Destroy', listing_app_url(@listing, app), confirm: 'Are you sure?', method: :delete %></td>
  </tr>   
<% end %>
</table>

<br />

<%= link_to 'New App', new_listing_app_path %></div>
